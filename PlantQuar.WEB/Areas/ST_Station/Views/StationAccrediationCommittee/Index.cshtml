    $(function () {
        $('#selectApprove').change(function () {
            btn_Approved_Click();
        });
        $('#stationcode').keyup(function () {
            btn_Approved_Click();
        });
        $('#activityType').change(function () {
            btn_Approved_Click();
        });
        $('#requestid').change(function () {

            radio_Approved_Click(1);
        });
        $('#requestidxx').change(function () {

            radio_Approved_Click(2);
        });
        $('#requestidx').change(function () {

