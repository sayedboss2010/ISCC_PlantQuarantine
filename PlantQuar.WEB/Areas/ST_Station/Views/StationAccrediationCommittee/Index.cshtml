    function btn_Approved_Click() {
        ////debugger;
        var stationcode = $("#stationcode").val();
        if (stationcode == null || stationcode == "" || stationcode == 'undefined') {
            stationcode = "";
        }
        var html = '';
        var html_Status = '';
        var j = 0;
        var Status = $("#selectApprove").val();
        var activityType = $("#activityType").val();
        if (Status != 'choose') {
            $.getJSON('/StationAccrediationCommittee/getFarmCommitteeData', { stationcode: stationcode, Status: Status, stationActivityType: activityType }, function (data, succ) {
                console.log(data)
                $.each(data, function (i, item) {
                    stationId = data[i].stationId;
                    var Creation_Date = new Date(parseInt(data[i].station_Creation_Date.substr(6, 13)));
                    j = i + 1;
                    html += "<tr id='stationId_" + stationId + "'><td>" + j
                        + "</td><td>" + data[i].Accreditation_Type_ID
                        + "</td><td hidden='hidden'>" + data[i].station_Name
                        + "</td><td>" + data[i].StationCode
                        + "</td><td>" + Creation_Date.toLocaleDateString('en-GB')
                        + "</td><td>" + data[i].StationActivity_Name + "</td><td hidden='hidden'>--</td><td hidden='hidden'>--</td>";
                    html += "<td><a href='/Station_Pages/StationDetails/Index?stationId=" + item.stationId + "' >@PlantQuar.WEB.Language.Resource_data1.Details</a></td>";


                    if (data[i].Station_IsActive == 1) {
                        if (data[i].Request_IsActive == null) {
                            html += "<td>تم الموافقة على المحطة ولم يتم النظر الى الاعتماد</td>";
                        }
                        else if (data[i].Request_IsActive == false) {
                            html += "<td>تم الموافقة على المحطة  وتم رفض الاعتماد</td>";
                        }
                        else if (data[i].Request_IsActive == true) {
                            /*  html += "<td>تم الموافقة على الاعتماد</td>";*/
                            if (data[i].Station_Accreditation_IsPaid) {
                                /* تشكيل اول مره*/
                                if (data[i].station_Committee_STATUS == null) {
                                    html += "<td><a href='/Committees/Committee_Station/Index?Station_Requst_ID=" + data[i].Station_Accreditation_ID + "&Accreditation_Type_ID=" + data[i].Accreditation_Type_ID + "'>تشكيل لجنة</a></td>";
                                }
                                //else if (data[i].)
                                else if (data[i].station_Committee_STATUS == false) {
                                    if (data[i].station_Committee_IsApproved == null) {
                                        html += "<td>تم تشكيل لجنة وفي انتظار رد العميل</td>";
                                    }
                                    else if (data[i].station_Committee_IsApproved == true && data[i].station_Committee_IsPaid == null) {
                                        html += "<td>تم تشكيل لجنة وتمت الموافقة وفي انتظار دفع العميل</td>";
                                    }
                                    else if (data[i].station_Committee_IsApproved == true && data[i].station_Committee_IsPaid == true) {
                                        html += "<td>تم تشكيل لجنة وتمت الموافقة والدفع وفى انتظار عمل الاندريد</td>";
                                    }
                                    else if (data[i].station_Committee_IsApproved == true) {
                                        html += "<td>@PlantQuar.WEB.Language.Resource_data1.committeAccepted ولم يتم نزول الاندريد</td>";
                                    }
                                        /* تشكيل رفض العميل*/
                                    else if (data[i].station_Committee_IsApproved == false) {
                                        html += "<td>تم تشكيل لجنة وتم رفض العميل  <a href='/Committees/Committee_Station/Index?Station_Requst_ID=" + data[i].Station_Accreditation_ID + "&Accreditation_Type_ID=" + data[i].Accreditation_Type_ID + "'>تشكيل لجنة</a></td>";

                                    }
                                    /* الوقت انتهي ولم يتم نزول الاندرويد*/
                                        else if (data[i].station_Committee_Delegation_int == 0) {
                                            html += "<td>تم تشكيل لجنة ولم يتم نزول الاندريد  <a href='/Committees/Committee_Station/Index?Station_Requst_ID=" + data[i].Station_Accreditation_ID + "&Accreditation_Type_ID=" + data[i].Accreditation_Type_ID + "'>تشكيل لجنة</a></td>";
                                    }

                                      /* تم رفض اللجنة*/
                                     if (data[i].station_Committee_Is_Cancel == true ) {
                                        html += "<td>تم تشكيل لجنة وتعذر عمل اللجنة  <a href='/Committees/Committee_Station/Index?Station_Requst_ID=" + data[i].Station_Accreditation_ID + "&Accreditation_Type_ID=" + data[i].Accreditation_Type_ID + "'>تشكيل لجنة</a></td>";

                                    }


                                }
                                else {
                                    if (data[i].station_Committee_STATUS == true && data[i].station_Committee_Is_Cancel != true) {
                                        html += "<td>@PlantQuar.WEB.Language.Resource_data1.committeeWorkEnded <a href='/Station_Pages/Station_Final_Result/Index?stationId=" + data[i].stationId + "'>عرض النتايج</a></td>";
                                    }
                                    else if (data[i].station_Committee_Is_Cancel == true) {
                                        html += "<td>تم تشكيل لجنة وتعذر عمل اللجنة  <a href='/Committees/Committee_Station/Index?Station_Requst_ID=" + data[i].Station_Accreditation_ID + "&Accreditation_Type_ID=" + data[i].Accreditation_Type_ID + "'>تشكيل لجنة</a></td>";

                                    }
                                }
                            }
                            else {
                                html += "<td> تم الموافقة على الاعتماد  و لم يتم الدفع </td>";
                            }
                        }

                    }
                    else if (data[i].Station_IsActive == 0) {
                        html += "<td>تم رفض المحطة</td>";
                    }
                    else {
                        html += "<td>لم يتم النظر</td>";
                    }
                    /*html += html_Status;*/
                    html += "</tr>";
                });
                $("#tblCheckLst tbody").empty();
                $("#tblCheckLst tbody").append(html);
            });
        }
    }


