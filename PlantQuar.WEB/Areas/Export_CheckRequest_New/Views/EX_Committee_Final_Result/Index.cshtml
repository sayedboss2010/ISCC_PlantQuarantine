        // Function to remove the row and send an AJAX request to delete from the server For Vissa
        function removeRow(visaId) {
            // Confirm deletion
            if (!confirm("هل أنت متأكد من أنك تريد حذف هذه التأشيرة؟")) {
                return;
            }

            // Find the row by its ID
            var row = document.getElementById('row-' + visaId);

            if (row) {
                // Remove the row from the table
                row.parentNode.removeChild(row);
            }

            // Send an AJAX request to delete the record from the server
            $.ajax({
                url: '/EX_Committee_Final_Result/DeleteVisa',   // Adjust the URL as needed
                type: 'POST',
                data: { id: visaId },
                success: function (response) {
                    if (response.success) {
                        alert('تم حذف التأشيرة بنجاح.');
                        window.location.reload();
                    } else {
                        alert('فشل الحذف، حاول مرة أخرى.');
                    }
                },
                error: function () {
                    alert('حدث خطأ أثناء محاولة الحذف.');
                }
            });
        }

        // Function to remove the row and send an AJAX request to delete from the server For FinalResult
        function removeRowFinalresult(finalResultId) {
            // Confirm deletion
            if (!confirm("هل أنت متأكد من أنك تريد حذف هذه الموقف النهائي؟")) {
                return;
