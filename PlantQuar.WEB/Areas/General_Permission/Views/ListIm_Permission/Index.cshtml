<div id="request">

    @* التي تمت الموافقه عليها
    *@
    <div id="checkRequest_lst1" class="table-responsive" hidden="hidden">
        <table class="table table-bordered col-md-12" id="tblCheckLst1">
            <thead>
                <tr class="label-primary">
                    <th>@PlantQuar.WEB.Language.Resource_data1.Seq</th>
                    <th>@PlantQuar.WEB.Language.Resource_data1.ImPermissionNumber</th>

                    <th>@PlantQuar.WEB.Language.Resource_data1.ShortName_qualG</th>
                    <th>@PlantQuar.WEB.Language.Resource_data1.ImporterType</th>
                    <th>@PlantQuar.WEB.Language.Resource_data1.ImporterName2</th>
                    <th>@PlantQuar.WEB.Language.Resource_data1.ExportCountry</th>
                    <th>@PlantQuar.WEB.Language.Resource_data1.RequestDate</th>
                    <th>@PlantQuar.WEB.Language.Resource_data1.Details</th>
                    <th id="printData">@PlantQuar.WEB.Language.Resource_data1.Print</th>
                </tr>
            </thead>
            <tbody id="Renew1">
                @if (model != null)
                {
                    foreach (var item in model.Where(a => a.IsAcceppted == true))
                    {
                        <tr>
                            <td>@item.row_number </td>
                            <td> @item.ImPermission_Number.ToString().Split('.')[0]</td>
                            <td> @item.shortName</td>
                            <td>@item.ImporterTypeName</td>
                            <td>@item.ImporterName</td>
                            <td>@item.ExportCountryName</td>
                            <td>@item.Arrival_Date.ToString("dd/MM/yyyy")</td>
                            <td><a href='/General_Permission/Im_permissionDetail/Index?ImPermission_Number=@item.ImPermission_Number.ToString().Split('.')[0]'>@PlantQuar.WEB.Language.Resource_data1.Details</a></td>
                            @{
                                if (item.IsAcceppted == true)
                                {
                                    if (item.IsPaid == true)
                                    {
                                        if (ViewBag.CanPrint == true)
                                        {
                                            if (item.IS_Print_Ar == true && item.IS_Print_EN == true)
                                            {
                                                <td> تم الموافقه وتم الدفع وتمت الطباعه</td>
                                            }
                                            else
                                            {
                                                <td>تمت الموافقه وتم الدفع  <a href='/General_Permission/Im_PermissionRequestPrint/Index?ImPermission_Number=@item.ImPermission_Number.ToString().Split('.')[0] '>@PlantQuar.WEB.Language.Resource_data1.Print</a></td>
                                            }
                                        }
                                        else if (ViewBag.CanPrint == false)
                                        {
                                            <td> تم الموافقه وتم الدفع ولا يوجد صلاحيه للطباعه</td>
                                        }
                                        else
                                        {
                                            <td>تمت الموافقه وتم الدفع  <a href='/General_Permission/Im_PermissionRequestPrint/Index?ImPermission_Number=@item.ImPermission_Number.ToString().Split('.')[0]'>@PlantQuar.WEB.Language.Resource_data1.Print</a></td>
                                        }
                                    }
                                    else if (item.IsPaid == null)
                                    {
                                        <td> تمت الموافقه ولم يتم الدفع</td>
                                    }
                                }
                                else if (item.IsAcceppted == false)
                                {
                                    <td> تم رفض الطلب  </td>
                                }
                                else
                                {
                                    <td>  لم يتخذ اجراء علي الإذن  </td>
                                }
                            }
                        </tr>
                    }
                }
            </tbody>
        </table>
        <br />

    </div>


