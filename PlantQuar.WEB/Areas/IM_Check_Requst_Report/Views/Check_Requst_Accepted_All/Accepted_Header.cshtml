<script type="text/javascript">
        $(document).ready(function () {


             $.ajax(
                {
                    url: '@Url.Action("Accepted_Header", "Check_Requst_Accepted_All", new { area = "IM_Check_Requst_Report" })',
                    type: 'Post',
                    success: function (data) {
                        $('#itemTypeLst').empty();
                        $('#idnnnn').val() = data.Lst.Customs_Certificate_Number;

                        $.each(data.Options, function (i, index) {
                            $('#itemTypeLst').append($('<option />').val(index.Value).text(index.DisplayText));
                        }); //each
                    } //success
                 });//ajax

         

        });
</script>




