@{
    ViewBag.Title = "موقف طلبات الفحص الواردة";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
    var model = Model;
}

@section Title{
    <h2 class="ls-top-header">موقف طلبات الفحص الواردة</h2>
}

@section Path{
    <li style="position: relative;right: 28px;">فحص</li>
    <li class="active" style="right: 29px; position: relative;">Im Check Request List</li>
}


@*الكل*@
<div id="checkRequest_lst4" class="table-responsive">
    <table class="table table-bordered col-md-12" id="tblCheckLst">
        <thead>
            <tr class="label-primary">
                <th>م</th>
                <th>م</th>
                <th>@PlantQuar.WEB.Language.Resource_data1.Outlet</th>
                <th>العدد الكلي للطبات</th>
                <th>الطلبات المقبولة</th>
                <th>الطلبات المرفوضة</th>
                <th>طلبات لم يتم النظر اليها</th>
                <th>الطلبات المغلقة</th>
                @*<th>الطلبات الغير مغلقة</th>
                <th>جاري العمل عليها</th>*@
            </tr>
        </thead>
        <tbody>
            @if (model != null)
            {
                var i = 1;
                foreach (var item in model)
                {
                    <tr>
                        <td>@i</td>
                        <td>@item.outlet_ID</td>
                        <td>@item.outlet_Name </td>
                        <td>@item.CheckRequest_Count_all @if (@item.CheckRequest_Count_all > 0) {<a href="/Im_CheckRequests/List_ImCheckRequest/Index?outlet_ID=@item.outlet_ID&&CheckRequest=1">عرض</a> }  </td>
                        <td>@item.CheckRequest_Count_true  @if (@item.CheckRequest_Count_true > 0) {<a href="/Im_CheckRequests/List_ImCheckRequest/Index?outlet_ID=@item.outlet_ID&&CheckRequest=2">عرض</a>}</td>
                        <td>@item.CheckRequest_Count_false @if(@item.CheckRequest_Count_false >0){ <a href="/Im_CheckRequests/List_ImCheckRequest/Index?outlet_ID=@item.outlet_ID&&CheckRequest=2">عرض</a>}</td>
                        <td>@item.CheckRequest_Count_nulls @if (@item.CheckRequest_Count_nulls > 0) {<a href="/Im_CheckRequests/List_ImCheckRequest/Index?outlet_ID=@item.outlet_ID&&CheckRequest=3">عرض</a>}</td>
                        <td>@item.Closed_Request_true  @if (@item.Closed_Request_true > 0){<a href="/Im_CheckRequests/List_ImCheckRequest/Index?outlet_ID=@item.outlet_ID&&CheckRequest=4">عرض</a>}</td>
                        @*<td>@item.Closed_Request_false  @if (@item.Closed_Request_false > 0){<a href="/Im_CheckRequests/List_ImCheckRequest/Index?outlet_ID=@item.outlet_ID&&CheckRequest=1">عرض</a>}</td>
                        <td>@item.Closed_Request_nulls  @if (@item.Closed_Request_nulls > 0){<a href="/Im_CheckRequests/List_ImCheckRequest/Index?outlet_ID=@item.outlet_ID&&CheckRequest=1">عرض</a>}</td>*@
                    </tr>
                    // @count= count + 1;
                    i++;
                }
            }
        </tbody>
    </table>
</div>


