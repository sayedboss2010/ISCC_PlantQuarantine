@model PlantQuar.DTO.DTO.Export_Constrains.Ex_ConstrainActivationProcedureDTO


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

@section Title{
    <h2 class="ls-top-header">@PlantQuar.WEB.Language.Resource_data1.ConstrainActivationProcedure </h2>
}

@section Path{
    <li>@PlantQuar.WEB.Language.Resource_data1.CountryConstrain</li>
    <li class="active">@PlantQuar.WEB.Language.Resource_data1.ConstrainActivationProcedure</li>
}

<style>
    select.form-control:not([size]):not([multiple]) {
        height: 3.7rem;
    }

    .row {
        width: 100%;
    }

    .ta1 {
        display: inline-block;
    }

    blockquote {
        margin-bottom: 50px !important;
    }
</style>
@using (Html.BeginForm("constrainSave", "Ex_ConstrainActivationProcedure", FormMethod.Post, new { @enctype = "multipart/form-data", @id = "ex_ConstrainForm" }))
{
    <div class="row form-box">
        <div class="col-md-12">

            <div class="col-md-4" id="countriesDiv">
                <label class="control-label">  @PlantQuar.WEB.Language.Resource_data1.ImportCountry </label>
                @Html.DropDownListFor(m => m.ownerImportId, new SelectList(ViewBag.CountriesLst, "Value", "DisplayText"), new { @class = "form-control", @id = "countriesLstImport" })
            </div>
            <div class="col-md-4" id="countriesDiv">
                <label class="control-label">
                    @PlantQuar.WEB.Language.Resource_data1.TransitCountry
                </label>
                @Html.DropDownListFor(m => m.ownerTransitId, new SelectList(ViewBag.CountriesLst, "Value", "DisplayText"), new { @class = "form-control", @id = "countriesLstTransit" })
            </div>

        </div>
    </div>
    <div style="border: 1px dashed #cbc5ba; margin: 13px;"></div>

    <div class="row">
        <div class="col-md-12">
            <div class="tab" role="tabpanel">
                <!-- Nav tabs -->
                <ul class="nav nav-tabs" role="tablist">
                    <li role="presentation" class="active">
                        <a data-id="1" href="#PlantTab" aria-controls="profile" role="tab" data-toggle="tab">@PlantQuar.WEB.Language.Resource_data1.Plant</a>
                    </li>
                    <li role="presentation">
                        <a data-id="2" href="#ProductTab" aria-controls="profile" role="tab" data-toggle="tab">@PlantQuar.WEB.Language.Resource_data1.Product</a>
                    </li>
                    <li role="presentation">
                        <a data-id="3" href="#AliveLiableItemsTab" aria-controls="profile" role="tab" data-toggle="tab">@PlantQuar.WEB.Language.Resource_data1.LiableAlive</a>
                    </li>


                    <li role="presentation">
                        <a data-id="4" href="#NotAliveLiableItemsTab" aria-controls="profile" role="tab" data-toggle="tab">@PlantQuar.WEB.Language.Resource_data1.LiableAliveNot</a>
                    </li>
                </ul>
                <!-- Tab panes -->
                <div class="tab-content tabs" style="min-height:433px;">
                    <div role="tabpanel" class="tab-pane fade in active" id="PlantTab">
                        @Html.Partial("~/Areas/Export_ConstrainActivationProcedure/Views/PlantConstrain_ActivationProcedure/PlantConstrain_ActivationProcedure.cshtml")
                    </div>
                   
                </div>
            </div>
        </div>
    </div>
    <br />

    @* *****************************  SAVE  ************************* *@
    <div class="row">
        <div class="col-md-11"></div>
        <input type="submit" id="btnSavec" name="btnSave" value="@PlantQuar.WEB.Language.Resource_data1.Save" class="btn ls-light-green-btn btn-round btn-lg col-md-1 col-sm-4" style="float:left" />
    </div>
}



@*/**********************************/*@
